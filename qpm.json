{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Pause Remapper",
    "id": "PauseRemapper",
    "version": "1.2.3",
    "url": null,
    "additionalData": {
      "overrideSoName": "libPauseRemapper.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?"
      ],
      "logcat": [
        "pwsh ./scripts/start-logging.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1"
      ]
    },
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodIncludeFiles": [
      "cover.gif"
    ],
    "qmodOutput": "./PauseRemapper.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.1.0",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.4",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "paper",
      "versionRange": "^3.6.3",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^3700.0.0",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^1.4.2",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.42",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.6",
      "additionalData": {}
    },
    {
      "id": "lapiz",
      "versionRange": "^0.2.11",
      "additionalData": {}
    }
  ]
}
